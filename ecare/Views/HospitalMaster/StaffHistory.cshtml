@model IEnumerable<ecare.Models.Staff>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_HospitalLayoutPage.cshtml";
}


<h4>Staff History</h4>
<hr />
<p>
    @using (Html.BeginForm("StaffHistory", "HospitalMaster", FormMethod.Get))
    {
        <input type="text" name="search" />
        <input type="submit" value="Search" name="submit" />
    }
</p>
<div class="table-responsive ta">

    <table id="myTable" class="table table-bordered table-sm table-hover">
        <tr class="bg-custom text-white thead-light">
            <th>
                @Html.DisplayNameFor(model => model.StaffId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StaffName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StaffAddress)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.StaffPhone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StaffEmail)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ActionType)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.StaffId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StaffName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StaffAddress)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.StaffPhone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StaffEmail)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ActionType)
        </td>

 
    </tr>
        }

    </table>

</div>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>








