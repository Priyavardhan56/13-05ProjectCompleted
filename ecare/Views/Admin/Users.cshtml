@model IEnumerable<ecare.Models.Patient>

@{
    ViewBag.Title = "Users";
    Layout = "~/Views/Shared/_AdminLayoutPage.cshtml";
}
<h4>Users List</h4>
<hr />
<p>
    @using (Html.BeginForm("Users", "Admin", FormMethod.Get))
    {
        <input type="text" name="Search" />
        <input type="submit" value="Search" name="submit" />
    }
</p>
<div class="table-responsive ta">
    <p>
        @Html.ActionLink("Create New", "AddUsers", "Admin", new { @class = "btn btn-custom text-white btn-sm" })
    </p>
    <table id="myTable" class="table table-bordered table-sm table-hover">
        <tr class="bg-custom text-white thead-light">
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Address)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.Phone)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.EntryDateTime)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EntryBy)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.Phone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.EntryDateTime)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EntryBy)
        </td>
    </tr>
        }

    </table>

</div>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>









